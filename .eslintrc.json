{
  "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "ignorePatterns": ["node_modules/*", ".next/*", ".out/*"],
    "extends": ["standard-with-typescript", "plugin:prettier/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off"
    }
}
